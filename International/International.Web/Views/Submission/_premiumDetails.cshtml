@model International.BusinessEntities.Models.PremiumDetailModel 
@{
    var isView = (bool)ViewBag.isView;
}  
    @if (!isView)
    {
<div class="container-fluid  dt-overflow" ng-init="PremiumDetailInit()">
        
    <div class="bg-primary clearfix">
        <h4 class="pull-left">Premium Details</h4> 
        <button style="margin-top:3px;" class="btn btn-info pull-right" ng-show="submissionModel.CurrentStatusId==enums.CurrentStatusEnum.Bound || submissionModel.CurrentStatusId==enums.CurrentStatusEnum.ReEntry" ng-click="OnAddLayerClickPremium()"><i class="fa fa-plus-circle"></i> Add Layer</button>
    </div>
    
    <form id="PremiumDetailForm" name="PremiumDetailForm">
        <div class="td-space clearfix">
            <div ng-if="currentprocess == (enums.SubmissionProcess.CreateAmendment) || currentprocess == (enums.SubmissionProcess.EditAmendment)">
            <div class="row">
                <div class="col-md-7 form-group">
                        <label class="col-md-4" for="name">Premium Type </label>
                    <div class="col-md-7">
                        <div class="input-group premimum">
                            <span style="margin-right:50px;">
                                    @Html.RadioButtonFor(model => model.PremiumType, "PT_AP", new
                                               {
                                                   data_ng_model = "submissionModel.PremiumDetail.PremiumType",
                                                   data_ng_change = "setPremimumType();",                                                  
                                                   validation_rules = "",
                                                   status = "{{submissionModel.CurrentStatusId}}",
                                                   identifier = "PremimumType"
                                               }) AP (Additional Premium)
                            </span>
                            <span>
                                    @Html.RadioButtonFor(x => x.PremiumType, "PT_RP", new
                                               {
                                                   data_ng_model = "submissionModel.PremiumDetail.PremiumType",
                                                   data_ng_change = "setPremimumType();",                                                   
                                                   validation_rules = "",
                                                   status = "{{submissionModel.CurrentStatusId}}",
                                                   identifier = "PremimumType"
                                               })RP (Return Premium)
                            </span>

                        </div>
                            <div>@Html.ValidationMessageFor(model => model.PremiumType)</div>
                    </div>

                </div>

            </div>
            <hr>
            </div>
            <div class="row">
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Exchange rate as on </label>
                    <div class="col-md-5">
                        <div class="input-group date">
                            @Html.TextBoxFor(model => model.ExchangeRateDate, new
                            {
                                @class = "form-control date",
                                date_time_picker = "",
                                data_ng_model = "submissionModel.PremiumDetail.ExchangeRateDate",
                                mindate = "Jan-01-2013",
                                maxdate = "Dec-31-2040",
                                validation_rules = "",
                                status = "{{submissionModel.CurrentStatusId}}",
                                identifier = "ExchangeRateDate",
                                ng_change = "ExchangeRateDateChange()"
                            })
                            <span class="input-group-addon" id="basic-addon2"><i class="fa fa-calendar"></i></span>
                        </div>
                        @Html.ValidationMessageFor(model => model.ExchangeRateDate)
                    </div>
                </div>
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">(%) of Layer</label>
                    <div class="col-md-5">

                        @Html.TextBoxFor(model => model.LayerPercent, new
                       {
                           data_ng_model = "submissionModel.PremiumDetail.LayerPercent",
                           valid_Decimal_Number2 = "",
                           maxlength = 16,
                           placeholder = "(%) of Layer",
                           @class = "form-control",
                           ng_change = "CalcOriginalLimit()",
                           validation_rules = "",
                           status = "{{submissionModel.CurrentStatusId}}",
                           identifier = "LayerPercent",
                           autocomplete = "off"
                       })
                        @Html.ValidationMessageFor(model => model.LayerPercent)
                    </div>
                </div>
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Policy Commission (%)</label>
                    <div class="col-md-5">
                        @Html.TextBoxFor(model => model.PolicyCommissionPercent, new
                       {
                           data_ng_model = "submissionModel.PremiumDetail.PolicyCommissionPercent",
                           valid_Decimal_Number2 = "",
                           maxlength = 16,
                           placeholder = "Policy Commission",
                           @class = "form-control",
                           ng_change = "CalcTransactionalPolicyCommission()",
                           validation_rules = "",
                           status = "{{submissionModel.CurrentStatusId}}",
                           identifier = "PolicyCommissionPercent",
                           autocomplete = "off"
                       })
                        @Html.ValidationMessageFor(model => model.PolicyCommissionPercent)
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Original Currency</label>
                    <div class="col-md-5">
                        <select class="form-control" ng-model="submissionModel.PremiumDetail.OriginalCurrencyCode"
                                ng-options="option.Value as option.Text for option in submissionListModel.OriginalCurrencyList"
                                data-val="true"
                                data-val-required="Please select Original Currency"
                                name="OriginalCurrencyCode" id="OriginalCurrencyCode" ng-change=""
                                validation_rules status="{{submissionModel.CurrentStatusId}}" identifier="OriginalCurrencyCode">
                            <option value="">- Select  -</option>
                        </select>
                        @Html.ValidationMessageFor(model => model.OriginalCurrencyCode)
                    </div>
                </div>
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Instalments (Yes/No)</label>
                    <div class="col-md-5">
                        <select class="form-control" id="Installment" name="Installment" ng-model="submissionModel.PremiumDetail.Installment"
                                validation_rules status="{{submissionModel.CurrentStatusId}}" identifier="Instalment"                                
                                data-val-required="Instalments (Yes/No) is required">
                            <option value="">-- Select --</option>
                            <option value="Yes">Yes</option>
                            <option value="No">No</option>
                        </select>
                        @Html.ValidationMessageFor(model => model.Installment)
                    </div>
                </div>
            </div>
            <hr>
            <div class="row">
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Layer Limit in Original Currency</label>
                    <div class="col-md-5">
                        @Html.TextBoxFor(model => model.OriginalLayerLimit, new
                       {
                           data_ng_model = "submissionModel.PremiumDetail.OriginalLayerLimit",
                           valid_Decimal_Number2 = "",
                           maxlength = 16,
                           placeholder = "Layer Limit in Original Currency",
                           @class = "form-control",
                           ng_change = "OnOriginalLayerLimitChange()",
                           validation_rules = "",
                           status = "{{submissionModel.CurrentStatusId}}",
                           identifier = "OriginalLayerLimit",
                           autocomplete = "off"
                       })
                        @Html.ValidationMessageFor(model => model.OriginalLayerLimit)
                    </div>
                </div>
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Limit in Original Currency &nbsp; </label>
                    <div class="col-md-5">
                        <input type="text" readonly placeholder="Limit in Original Currency" ng-model="OriginalLimit"
                               value="" class="form-control">
                    </div>
                </div>
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Attachment Point in Original Currency &nbsp; 
                    <input type="checkbox" ng-change="CHKOriginalAttachmentPointChange()" ng-model="submissionModel.PremiumDetail.IsOriginalAttachmentPoint" 
                            validation_rules status = "{{submissionModel.CurrentStatusId}}" identifier = "OriginalAttachmentPoint">
                    </label>
                    <div class="col-md-5">
                        @Html.TextBoxFor(model => model.OriginalAttachmentPoint, new
                        {
                           data_ng_model = "submissionModel.PremiumDetail.OriginalAttachmentPoint",
                           valid_decimal_number2 = "",
                           maxlength = 16,
                           placeholder = "Attachment Point in Original Currency",
                           @class = "form-control",
                           ng_change = "CalcTransactionalAttachmentPoint()",
                           ng_disabled = "submissionModel.PremiumDetail.IsOriginalAttachmentPoint",
                           validation_rules = "",
                           status = "{{submissionModel.CurrentStatusId}}",
                           identifier = "OriginalAttachmentPoint",
                           ng_show="!submissionModel.PremiumDetail.IsOriginalAttachmentPoint",
                           autocomplete = "off"
                       })
                        <input type="text" class="form-control" readonly value="Not Available" ng-show="submissionModel.PremiumDetail.IsOriginalAttachmentPoint" />
                        @Html.ValidationMessageFor(model => model.OriginalAttachmentPoint)
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Transactional Currency</label>
                    <div class="col-md-5">
                        <select class="form-control" ng-model="submissionModel.PremiumDetail.TransactionalCurrencyCode"
                                ng-options="option.Value as option.Text for option in submissionListModel.TransactionalCurrencyList"
                                data-val="true"
                                data-val-required="Please select Transactional Currency"
                                name="TransactionalCurrencyCode" id="TransactionalCurrencyCode" ng-change=""
                                validation_rules status="{{submissionModel.CurrentStatusId}}" identifier="TransactionalCurrencyCode">
                            <option value="">- Select  -</option>
                        </select>
                        @Html.ValidationMessageFor(model => model.TransactionalCurrencyCode)
                    </div>
                </div>
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Conversion Rate from Original Currency to Transactional Currency</label>
                    <div class="col-md-5">
                        @Html.TextBoxFor(model => model.ConversionRateToTransactional, new
                       {
                           data_ng_model = "submissionModel.PremiumDetail.ConversionRateToTransactional",
                           valid_Decimal_Number5 = "",
                           maxlength = 23,
                           placeholder = "Conversion Rate from Original Currency to Transactional Currency",
                           @class = "form-control",
                           ng_change = "OnConversionRateToTransactionalChange(submissionModel.PremiumDetail.ConversionRateToTransactional)",
                           validation_rules = "",
                           status = "{{submissionModel.CurrentStatusId}}",
                           identifier = "ConversionRateToTransactional",
                           autocomplete = "off"
                       })
                        @Html.ValidationMessageFor(model => model.ConversionRateToTransactional)

                    </div>
                </div>
            </div>
            <hr>
            <div class="row">
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Layer Limit in Transactional Currency</label>
                    <div class="col-md-5">
                        <input type="text" convert-nan readonly placeholder="Layer Limit in Transactional Currency" valid_decimal_number2 ng-model="TransactionalLayerLimit" value="" class=" form-control">
                    </div>
                </div>
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Limit in Transactional Currency</label>
                    <div class="col-md-5">
                        <input type="text" convert-nan readonly placeholder="Limit in Transactional Currency" valid_decimal_number2 ng-model="TransactionalLimit" value="" class=" form-control">
                    </div>
                </div>
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Attachment Point in Transactional Currency</label>
                    <div class="col-md-5">
                        <input type="text" convert-nan readonly placeholder="Attachment Point in Transactional Currency" valid_decimal_number2 ng-model="TransactionalAttachmentPoint" value="" class="form-control">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">SIR in Transactional Currency</label>
                    <div class="col-md-5">
                        @Html.TextBoxFor(model => model.TransactionalSIR, new
                       {
                           data_ng_model = "submissionModel.PremiumDetail.TransactionalSIR",
                           valid_Decimal_Number2 = "",
                           maxlength = 20,
                           placeholder = "SIR in Transactional Currency",
                           @class = "form-control",
                           validation_rules = "",
                           status = "{{submissionModel.CurrentStatusId}}",
                           identifier = "TransactionalSIR",
                           autocomplete = "off"
                       })
                        @Html.ValidationMessageFor(model => model.TransactionalSIR)

                    </div>
                </div>
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Deductible in Transactional Currency</label>
                    <div class="col-md-5">
                        @Html.TextBoxFor(model => model.TransactionalDeductible, new
                       {
                           data_ng_model = "submissionModel.PremiumDetail.TransactionalDeductible",
                           valid_Decimal_Number2 = "",
                           maxlength = 20,
                           placeholder = "Deductible in Transactional Currency",
                           @class = "form-control",
                           validation_rules = "",
                           status = "{{submissionModel.CurrentStatusId}}",
                           identifier = "TransactionalDeductible",
                           autocomplete = "off"
                       })
                        @Html.ValidationMessageFor(model => model.TransactionalDeductible)
                    </div>
                </div>
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Gross Premium in Transactional Currency</label>
                    <div class="col-md-5">
                        @Html.TextBoxFor(model => model.TransactionalGrossPremium, new
                       {
                           data_ng_model = "submissionModel.PremiumDetail.TransactionalGrossPremium",
                           valid_Decimal_Number2 = "",
                           maxlength = 20,
                           placeholder = "Gross Premium in Transactional Currency",
                           @class = "form-control",
                           ng_change = "OnTransactionalGrossPremiumChange()",
                           validation_rules = "",
                           status = "{{submissionModel.CurrentStatusId}}",
                           identifier = "TransactionalGrossPremium",
                           autocomplete = "off",
                           ng_blur = "setPremimumType()"
                       })
                        @Html.ValidationMessageFor(model => model.TransactionalGrossPremium)
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Policy Commission in Transactional Currency</label>
                    <div class="col-md-5">
                        <input type="text" convert-nan readonly placeholder="Policy Commission in Transactional Currency" valid_decimal_number2 ng-model="TransactionalPolicyCommission" value="" class=" form-control">
                    </div>
                </div>
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Collections in Transactional Currency</label>
                    <div class="col-md-5">
                        @Html.TextBoxFor(model => model.TransactionalCollections, new
                       {
                           data_ng_model = "submissionModel.PremiumDetail.TransactionalCollections",
                           valid_Decimal_Number2 = "",
                           maxlength = 20,
                           placeholder = "Collections in Transactional Currency",
                           @class = "form-control",
                           ng_change = "CalcTransactionalNetPremium()",
                           validation_rules = "",
                           status = "{{submissionModel.CurrentStatusId}}",
                           identifier = "TransactionalCollections",
                           autocomplete = "off"
                       })
                        @Html.ValidationMessageFor(model => model.TransactionalCollections)
                    </div>
                </div>
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Deductions in Transactional Currency</label>
                    <div class="col-md-5">
                        @Html.TextBoxFor(model => model.TransactionalDeductions, new
                       {
                           data_ng_model = "submissionModel.PremiumDetail.TransactionalDeductions",
                           valid_Decimal_Number2 = "",
                           maxlength = 20,
                           placeholder = "Deductions in Transactional Currency",
                           @class = "form-control",
                           ng_change = "CalcTransactionalNetPremium()",
                           validation_rules = "",
                           status = "{{submissionModel.CurrentStatusId}}",
                           identifier = "TransactionalDeductions",
                           autocomplete = "off"
                       })
                        @Html.ValidationMessageFor(model => model.TransactionalDeductions)
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">GST (IN) (Transactional Currency)</label>
                    <div class="col-md-5">
                        @Html.TextBoxFor(model => model.TransactionalGSTIN, new
                       {
                           data_ng_model = "submissionModel.PremiumDetail.TransactionalGSTIN",
                           valid_Decimal_Number2 = "",
                           maxlength = 20,
                           placeholder = "GST (IN) (Transactional Currency)",
                           @class = "form-control",
                           validation_rules = "",
                           status = "{{submissionModel.CurrentStatusId}}",
                           identifier = "TransactionalGSTIN",
                           autocomplete = "off",
                           ng_change="CalcTransactionalNetPremiumFromBroker()"
                       })
                        @Html.ValidationMessageFor(model => model.TransactionalGSTIN)
                    </div>
                </div>
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">GST (OUT) (Transactional Currency)</label>
                    <div class="col-md-5">
                        @Html.TextBoxFor(model => model.TransactionalGSTOUT, new
                       {
                           data_ng_model = "submissionModel.PremiumDetail.TransactionalGSTOUT",
                           valid_Decimal_Number2 = "",
                           maxlength = 20,
                           placeholder = "GST (OUT) (Transactional Currency)",
                           @class = "form-control",
                           validation_rules = "",
                           status = "{{submissionModel.CurrentStatusId}}",
                           identifier = "TransactionalGSTOUT",
                           autocomplete = "off",
                           ng_change = "CalcTransactionalNetPremiumFromBroker()"
                       })
                        @Html.ValidationMessageFor(model => model.TransactionalGSTOUT)
                    </div>
                </div>
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">BHSI Net Premium in Transactional Currency</label>
                    <div class="col-md-5">
                        <input type="text" convert-nan readonly placeholder="Net Premium in Transactional Currency" valid_decimal_number2 ng-model="TransactionalNetPremium" value="" class="form-control">
                    </div>
                </div>
                
            </div>
            <div class="row">
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Net Premium From Broker in Transactional Currency</label>
                    <div class="col-md-5">
                        <input type="text" convert-nan readonly placeholder="Net Premium in Transactional Currency" valid_decimal_number2 ng-model="TransactionalNetPremiumFromBroker" value="" class="form-control">
                    </div>
                </div>
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Total Insured Value (TIV) in Transactional currency &nbsp; <input type="checkbox" ng-change="submissionModel.PremiumDetail.TransactionalTIV=0" ng-model="submissionModel.PremiumDetail.IsTransactionalTIV"></label>
                    <div class="col-md-5">
                        @Html.TextBoxFor(model => model.TransactionalTIV, new
                       {
                           data_ng_model = "submissionModel.PremiumDetail.TransactionalTIV",
                           valid_Decimal_Number2 = "",
                           maxlength = 20,
                           placeholder = "Total Insured Value (TIV) in Transactional currency",
                           @class = "form-control",                          
                           validation_rules = "",
                           status = "{{submissionModel.CurrentStatusId}}",
                           identifier = "TransactionalTIV",
                           product = "{{submissionModel.ProductLineTypeId}}",
                           ng_disabled = "submissionModel.PremiumDetail.IsTransactionalTIV",
                           autocomplete = "off",
                           ng_show = "!submissionModel.PremiumDetail.IsTransactionalTIV" 
                       })
                        <input type="text" class="form-control" readonly value="Not Available" ng-show="submissionModel.PremiumDetail.IsTransactionalTIV" />
                        @Html.ValidationMessageFor(model => model.TransactionalTIV)                        
                    </div>
                </div>
            </div>
            <hr>
            <div class="row">
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Jurisdictional Currency</label>
                    <div class="col-md-5">
                        <select class="form-control" ng-model="submissionModel.PremiumDetail.JurCurrencyCode"
                                ng-options="option.Value as option.Text for option in submissionListModel.JurCurrencyList"
                                data-val="true"
                                data-val-required="Please select Jurisdictional Currency"
                                name="JurCurrencyCode" id="JurCurrencyCode" ng-change=""
                                validation_rules status="{{submissionModel.CurrentStatusId}}" identifier="JurCurrencyCode">
                            <option value="">- Select  -</option>
                        </select>
                        @Html.ValidationMessageFor(model => model.JurCurrencyCode)
                    </div>
                </div>
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Conversion Rate from Transactional to Jurisdictional Currency</label>
                    <div class="col-md-5">
                        @Html.TextBoxFor(model => model.JurExchangeRate, new
                       {
                           data_ng_model = "submissionModel.PremiumDetail.JurExchangeRate",
                           valid_Decimal_Number5 = "",
                           maxlength = 23,
                           placeholder = "Conversion Rate in Jurisdictional",
                           @class = "form-control",
                           ng_change = "OnConversionRateJurChange()",
                           validation_rules = "",
                           status = "{{submissionModel.CurrentStatusId}}",
                           identifier = "ConversionRateToJurisdictional",
                           autocomplete = "off"
                       })
                        @Html.ValidationMessageFor(model => model.JurExchangeRate)
                    </div>
                </div>
            </div>
            <hr>
            <div class="row">
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Layer Limit in Jurisdictional Currency</label>
                    <div class="col-md-5">
                        <input type="text" ng-model="JurisdictionalLayerLimit" convert-nan readonly placeholder="Layer Limit in Jurisdictional Currency" value="{{JurisdictionalLayerLimit=(TransactionalLayerLimit*submissionModel.PremiumDetail.JurExchangeRate).toFixed(2)}}" class="form-control">
                    </div>
                </div>
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Limit in Jurisdictional Currency</label>
                    <div class="col-md-5">
                        <input type="text" ng-model="JurisdictionalLimit" convert-nan readonly placeholder="Limit in Jurisdictional Currency" value="{{JurisdictionalLimit=(TransactionalLimit*submissionModel.PremiumDetail.JurExchangeRate).toFixed(2)}}" class="form-control">
                    </div>
                </div>
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Attachment Point in Jurisdictional Currency</label>
                    <div class="col-md-5">
                        <input type="text" ng-model="JurAttachmentPoint" convert-nan readonly placeholder="Attachment Point in Jurisdictional Currency" valid_decimal_number2 value="{{JurAttachmentPoint=(TransactionalAttachmentPoint*submissionModel.PremiumDetail.JurExchangeRate).toFixed(2)}}" class="form-control">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">SIR in Jurisdictional Currency</label>
                    <div class="col-md-5">
                        <input type="text" ng-model="JurSIR" convert-nan readonly placeholder="SIR in Jurisdictional Currency" valid_decimal_number2 value="{{JurSIR=(submissionModel.PremiumDetail.TransactionalSIR*submissionModel.PremiumDetail.JurExchangeRate).toFixed(2)}}" class="form-control">
                    </div>
                </div>
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Deductible in Jurisdictional Currency</label>
                    <div class="col-md-5">
                        <input type="text" ng-model="JurDeductible" convert-nan readonly placeholder="Deductible in Jurisdictional Currency" valid_decimal_number2 value="{{JurDeductible=(submissionModel.PremiumDetail.TransactionalDeductible*submissionModel.PremiumDetail.JurExchangeRate).toFixed(2)}}" class="form-control">
                    </div>
                </div>
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Gross Premium in Jurisdictional Currency</label>
                    <div class="col-md-5">
                        <input type="text" ng-model="JurGrossPremium" convert-nan readonly placeholder="Gross Premium in Jurisdictional Currency" valid_decimal_number2 value="{{JurGrossPremium=(submissionModel.PremiumDetail.TransactionalGrossPremium*submissionModel.PremiumDetail.JurExchangeRate).toFixed(2)}}" class="form-control">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Policy Commission in Jurisdictional Currency</label>
                    <div class="col-md-5">
                        <input type="text" ng-model="JurPolicyCommission" convert-nan readonly placeholder="Policy Commission in Jurisdictional Currency" valid_decimal_number2 value="{{JurPolicyCommission=(TransactionalPolicyCommission*submissionModel.PremiumDetail.JurExchangeRate).toFixed(2)}}" class="form-control">
                    </div>
                </div>
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Collections in Jurisdictional Currency</label>
                    <div class="col-md-5">
                        <input type="text" ng-model="JurCollections" convert-nan readonly placeholder="Collections in Jurisdictional Currency" valid_decimal_number2 value="{{JurCollections=(submissionModel.PremiumDetail.TransactionalCollections*submissionModel.PremiumDetail.JurExchangeRate).toFixed(2)}}" class="form-control">
                    </div>
                </div>
                <div class="col-md-4 form-group">
                    <label class="col-md-7" for="name">Deductions in Jurisdictional Currency</label>
                    <div class="col-md-5">
                        <input type="text" ng-model="JurDeductions" convert-nan readonly placeholder="Deductions in Jurisdictional Currency" valid_decimal_number2 value="{{JurDeductions=(submissionModel.PremiumDetail.TransactionalDeductions*submissionModel.PremiumDetail.JurExchangeRate).toFixed(2)}}" class="form-control">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-4 form-group">
